
@{
    ViewBag.Title = "MsgConfirmarPedido";
}

@Html.AntiForgeryToken()
<div class="text-center form-horizontal">

    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })

    <div class="form-group">

        @if (ViewBag.ErrorPedido != null)
        {
            <h3>@Html.Label("", (string)ViewBag.ErrorPedido, new { @class = "label label-danger" })</h3>
        }
        <br />
        @Html.ActionLink("Volver", "ListaProPrep", "Pedido", new { @class = "btn btn-info" })

    </div>
</div>

