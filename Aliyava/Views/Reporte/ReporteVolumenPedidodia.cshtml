@model IEnumerable<Common.DTO.DtoReporteVolumenPedidodia>

@{
    ViewBag.Title = "ReporteVolumenPedidodia";
}

<h2>ReporteVolumenPedidodia</h2>


<table class="table">
    <tr>
        <th>
            Cantidad de  Pedidos
        </th>
        <th>
            Volumen
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.cantPedidos)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Volumen)
        </td>

    </tr>
}

</table>
