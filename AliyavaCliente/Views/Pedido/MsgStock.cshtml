
@{
    ViewBag.Title = "MsgStock";
}

@Html.AntiForgeryToken()
<div class="text-center form-horizontal">

    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })

    <div class="form-group">

        @if (ViewBag.ErrorStock != null)
        {
            <h3>@Html.Label("", (string)ViewBag.ErrorStock, new { @class = "label label-primary" })</h3>
        }
        <br />
        @Html.ActionLink("Inicio", "Index", "Home", new { @class = "btn btn-info" })

    </div>
</div>
