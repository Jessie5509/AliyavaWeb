
@{
    ViewBag.Title = "MsgErrorPedido";
}

@Html.AntiForgeryToken()
<div class="text-center form-horizontal">

    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })

    <div class="form-group">

        @if (ViewBag.Pedido != null)
        {
            <h3>@Html.Label("", (string)ViewBag.Pedido, new { @class = "label label-danger" })</h3>
        }
        <br />
        @Html.ActionLink("Volver", "CarritoView", "Pedido", new { @class = "btn btn-info" })

    </div>
</div>

